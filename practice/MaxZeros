import java.util.Arrays;

public class MaxZeroes {

    // Function to find the maximum number of zeroes
    public static int maxZeroes(int[] arr, int n) {
        int zeroCount = 0;  // Initialize the number of zeroes

        while (true) {
            // Find the minimum non-zero element
            int minNonZero = Integer.MAX_VALUE;
            boolean foundNonZero = false;

            for (int i = 0; i < n; i++) {
                if (arr[i] > 0) {
                    minNonZero = Math.min(minNonZero, arr[i]);
                    foundNonZero = true;
                }
            }

            // If no non-zero element found, break the loop
            if (!foundNonZero) {
                break;
            }

            // Subtract the minimum non-zero element from all non-zero elements
            for (int i = 0; i < n; i++) {
                if (arr[i] > 0) {
                    arr[i] -= minNonZero;
                    // If element becomes zero, increase the zero count
                    if (arr[i] == 0) {
                        zeroCount++;
                    }
                }
            }
        }

        return zeroCount;
    }

    public static void main(String[] args) {
        // Example usage
        int[] arr = {3, 4, 1, 2, 5};  // Input array
        int n = arr.length;

        // Find and print the maximum number of zeroes
        int result = maxZeroes(arr, n);
        System.out.println("Maximum number of zeroes: " + result);
    }
}
